{
  "name": "network-data",
  "author": {
    "name": "Diniden",
    "email": "diniden@outlook.com",
    "url": "http://diniden.com"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Diniden/network-data.git"
  },
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "clean": "devops clean",
    "dev-link": "ts-node --transpile-only bin/main dev-link",
    "dev": "devops dev",
    "pr": "devops pr --repoUrl \"https://github.com/Diniden/network-data\" --repoType git --masterBranch master",
    "release": "devops release",
    "storybook": "devops storybook",
    "ts-dev": "nodemon -e ts,tsx --watch \"lib/components/**/*\" --watch \"lib/util/**/*\" --watch \"lib/index.ts\" -x \"ts-node --transpile-only bin/main ts\"",
    "ts": "devops ts",
    "component": "devops component",
    "init": "devops init",
    "test": "devops test",
    "url": "devops url"
  },
  "devDependencies": {
    "@types/d3-force": "3.0.3",
    "@types/d3-hierarchy": "3.1.0",
    "@types/d3-scale": "4.0.2",
    "@types/random-seed": "0.3.3",
    "d3-force": "3.0.0",
    "d3-hierarchy": "3.1.2",
    "d3-scale": "4.0.2",
    "deltav": "3.0.0",
    "devops": "github:vega-studio/node-devops#semver:2.14.0",
    "random-seed": "0.3.0",
    "simple-data-provider": "1.1.6"
  },
  "greenkeeper": {
    "ignore": []
  },
  "eslintIgnore": [
    "**/*.min.js"
  ],
  "shader": {
    "ext": [
      "fs",
      "vs"
    ]
  },
  "type": "module",
  "native": {
    "xcode": {
      "tools": {
        "path": "/Applications/Xcode.app/Contents/Developer",
        "version": {
          "min": "2397",
          "max": "2397"
        }
      }
    }
  },
  "hosts": {
    "127.0.0.1": "local.network-data.com"
  },
  "url": {
    "dev": "https://local.network-data.com",
    "figma": "N/A",
    "repo": "https://github.com/Diniden/network-data"
  },
  "skeleton": {
    "answers": {
      "repoUrl": "https://github.com/Diniden/network-data",
      "repoType": "git",
      "masterBranch": "master",
      "figmaUrl": "N/A"
    }
  }
}
